{
  "version": 2,
  "name": "qwer-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/qwer/(.*)",
      "dest": "/public/qwer/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(styles\\.css|app\\.js)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js",
      "headers": {
        "Cache-Control": "public, s-maxage=300, stale-while-revalidate=600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/photos/refresh",
      "schedule": "0 */6 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GOOGLE_API_KEY": "@google_api_key",
    "GOOGLE_CX": "@google_cx"
  },
  "regions": [
    "sin1"
  ]
}